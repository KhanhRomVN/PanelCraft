{
  "search.exclude": {
    "**/venv": true,
    "**/.venv": true,
    "**/__pycache__": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.tox": true,
    "**/.cache": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true,
    "**/.git": true,
    "**/.idea": true,
    "**/.vscode": true,
